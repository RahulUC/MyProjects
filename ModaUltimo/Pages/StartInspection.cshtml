@page
@model StartInspectionModel
@{
}


<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <form method="post" style="margin-top: 50px;">
            
            <div class="form-group">
                <label asp-for="StartInspectionForm.StoreId" class="control-label"></label>
                <select asp-for="StartInspectionForm.StoreId" class ="form-control" 
                asp-items="ViewBag.StoreId">hello</select>
                <span asp-validation-for="StartInspectionForm.StoreId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StartInspectionForm.InventoryId" class="control-label"></label>
                <select asp-for="StartInspectionForm.InventoryId" class ="form-control" 
                asp-items="ViewBag.InventoryId">hello</select>
                <span asp-validation-for="StartInspectionForm.InventoryId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Start Inspection" class="btn btn-danger" />
            </div>
            
        </form>
    </div>
</div>
